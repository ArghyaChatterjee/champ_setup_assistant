<launch>
    <arg name="lite"  default="false" />

    <include file="$(find {{ robot_name }}_config)/launch/bringup.launch">
        <arg name="gazebo" value="true"/>
        <arg name="lite" value="$(arg lite)"/>
    </include>

    <include file="$(find champ_gazebo)/launch/gazebo.launch">
        <arg name="lite" value="$(arg lite)"/>
    </include>    
</launch>